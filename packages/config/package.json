{
  "name": "@gmacko/config",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./integrations": {
      "types": "./dist/integrations.d.ts",
      "default": "./src/integrations.ts"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./src/preview.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@gmacko/eslint-config": "workspace:*",
    "@gmacko/prettier-config": "workspace:*",
    "@gmacko/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@gmacko/prettier-config"
}
