{
  "name": "@gmacko/trpc-client",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsup src/index.ts --format esm --watch --dts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "superjson": "2.2.3"
  },
  "devDependencies": {
    "@gmacko/api": "workspace:*",
    "@gmacko/eslint-config": "workspace:*",
    "@gmacko/prettier-config": "workspace:*",
    "@gmacko/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.5.0",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@gmacko/api": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@gmacko/api": {
      "optional": true
    }
  },
  "prettier": "@gmacko/prettier-config"
}
