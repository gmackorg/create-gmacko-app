{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"],
      "cache": true
    },
    "test:watch": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "e2e": {
      "dependsOn": ["^build"],
      "outputs": [
        "playwright-report/**",
        "test-results/**",
        ".maestro/screenshots/**"
      ],
      "cache": false
    },
    "e2e:record": {
      "cache": false,
      "persistent": true
    },
    "e2e:studio": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "AUTH_DISCORD_ID",
    "AUTH_DISCORD_SECRET",
    "AUTH_REDIRECT_PROXY_URL",
    "AUTH_SECRET",
    "PORT",
    "NEXT_PUBLIC_PREVIEW",
    "PREVIEW_PR_NUMBER",
    "PREVIEW_DOMAIN",
    "PREVIEW_BASE_DOMAIN",
    "PREVIEW_DATABASE_URL",
    "PREVIEW_USE_SCHEMA_ISOLATION",
    "PREVIEW_SEND_EMAILS",
    "PREVIEW_ENABLE_ANALYTICS",
    "PREVIEW_BRANCH",
    "PREVIEW_COMMIT",
    "PREVIEW_DEPLOYED_AT",
    "GMACKO_API_URL",
    "GMACKO_API_KEY",
    "LOG_LEVEL",
    "SERVICE_NAME",
    "npm_package_version",
    "APP_ENV",
    "API_URL"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "VERCEL_GIT_COMMIT_REF",
    "VERCEL_GIT_COMMIT_SHA",
    "NEXT_RUNTIME",
    "npm_lifecycle_event"
  ]
}
