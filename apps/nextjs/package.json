    "@playwright/test": "^1.52.0",
    "@types/node": "catalog:",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "eslint": "catalog:",
    "jiti": "^2.5.1",
    "prettier": "catalog:",
